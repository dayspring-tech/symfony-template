{
    "name": "vagrant/symfony",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "3.4.*",
        "twig/extensions": "1.3.*",
        "symfony/assetic-bundle": "2.8.*",
        "symfony/swiftmailer-bundle": "^2.6.4",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/yaml": "^3.1",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.0.1",
        "incenteev/composer-parameter-handler": "^2.0",
        "propel/propel-bundle": "1.6.x-dev",
        "propel/propel1": "^1.6.8",
        "braincrafted/bootstrap-bundle": "2.2.*",
        "ekino/newrelic-bundle": "^1.3.2",
        "oneup/flysystem-bundle": "^1.11.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "lcobucci/jwt": "^3.1",
        "dayspring-tech/service-test-helper": "dev-symfony_3",
        "dayspring-tech/login-bundle": "dev-feature/symfony_3"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "phpunit/phpcov": "^4.0",
        "phpunit/phpunit": "^6.0",
        "symfony/phpunit-bridge": "^3.4",
        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd" : "2.*"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
