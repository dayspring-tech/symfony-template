<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="dayspring_security.user_provider" class="Dayspring\SecurityBundle\Security\User\DayspringUserProvider">
        </service>
        <service id="dayspring_security.username_password_authenticator" class="Dayspring\SecurityBundle\Security\UsernamePasswordAuthenticator">
            <argument type="service" id="service_container" />
        </service>
    </services>
</container>
